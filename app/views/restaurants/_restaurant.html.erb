<tr>
    <td>
        <%= restaurant.name %>
    </td>
    <td>
        <%= restaurant.cuisine %>
    </td>
    <td>
        <%= restaurant.rating %>
    </td>
    <td>
        <%= restaurant.accepts_10bis %>
    </td>
    <td>
        <%= restaurant.address %>
    </td>
    <td>
        <%= restaurant.max_delivery_time %>
    </td>
    <td>
        <%= link_to 'Show', restaurant %>
    </td>
    <td>
        <%= link_to 'Edit', edit_restaurant_path(restaurant) %>
    </td>
    <td>
        <%= link_to 'Destroy', restaurant, method: :delete, data: { confirm: 'Are you sure?' } %>
    </td>
</tr>